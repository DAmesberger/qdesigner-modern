{
  "name": "qdesigner-modern",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.2",
  "scripts": {
    "dev": "vite dev --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json && tsc",
    "setup": "pnpm install && pnpm supabase:start && pnpm supabase:wait && pnpm db:setup",
    "setup:fresh": "pnpm supabase:clean && pnpm setup",
    "start": "concurrently -n \"supabase,app\" -c \"yellow,cyan\" \"pnpm supabase:start\" \"pnpm dev\"",
    "supabase:start": "docker-compose up -d",
    "supabase:stop": "docker-compose down",
    "supabase:restart": "docker-compose restart",
    "supabase:logs": "docker-compose logs -f",
    "supabase:logs:auth": "docker-compose logs -f supabase-auth",
    "supabase:logs:kong": "docker-compose logs -f kong",
    "supabase:wait": "wait-on tcp:8000 tcp:54322 -t 30000",
    "supabase:clean": "docker-compose down -v",
    "db:setup": "pnpm db:migrate && pnpm db:seed",
    "db:migrate": "tsx scripts/db-migrate.ts",
    "db:seed": "tsx scripts/db-seed.ts",
    "db:reset": "docker-compose exec -T supabase-db psql -U postgres -c 'DROP SCHEMA IF EXISTS public CASCADE; CREATE SCHEMA public;' && pnpm db:setup",
    "db:create-test-user": "tsx scripts/create-test-user.ts",
    "db:psql": "docker-compose exec supabase-db psql -U postgres",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json && tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:unit": "vitest run src/**/*.test.ts tests/unit/**/*.test.ts",
    "test:unit:watch": "vitest",
    "test:integration": "vitest run tests/integration/**/*.test.ts",
    "test:integration:watch": "vitest tests/integration/**/*.test.ts",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:perf": "vitest run tests/performance/**/*.test.ts",
    "test:all": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
    "test:all:coverage": "pnpm test:coverage && pnpm test:e2e",
    "test:watch": "vitest",
    "test:setup": "docker-compose up -d supabase-db && pnpm supabase:wait && pnpm db:migrate",
    "test:teardown": "docker-compose down",
    "clean": "rm -rf dist .svelte-kit node_modules/.vite",
    "clean:all": "rm -rf dist .svelte-kit node_modules/.vite node_modules",
    "install:browsers": "playwright install",
    "prepare": "husky install || true"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.53.2",
    "@supabase/supabase-js": "^2.50.3",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.22.5",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^22.0.0",
    "@types/papaparse": "^5.3.16",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "dotenv": "^17.1.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.10.1",
    "fake-indexeddb": "^6.0.1",
    "isomorphic-dompurify": "^2.26.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.35.6",
    "svelte-check": "^4.2.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.7.0",
    "typescript": "~5.8.3",
    "vite": "^6.0.0",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.39.3",
    "@xyflow/svelte": "^1.2.2",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^4.1.0",
    "dexie": "^4.0.11",
    "exceljs": "^4.4.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "immer": "^10.1.1",
    "lucide-svelte": "^0.536.0",
    "marked": "^16.0.0",
    "mathjs": "^14.5.3",
    "monaco-editor": "^0.45.0",
    "nanoid": "^5.1.5",
    "papaparse": "^5.5.3",
    "svelte-dnd-action": "^0.9.63"
  }
}
